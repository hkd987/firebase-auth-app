{
  "rules": {
    ".read": "auth != null",
    "users": {
      "$uid": {
        ".write": "auth.uid == $uid",
      }
    },

    "all_statues": {
      ".indexOn": [
        "status_time_stamp"
      ]
    },
    "all_comments": {
      ".indexOn": [
        "comment_time_stamp"
      ]
    }
  }
}
